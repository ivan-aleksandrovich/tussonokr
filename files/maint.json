{
  "shiftly": [
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/current/",
      "rangetype": "last",
      "rangecount": 1,
      "action": "compress",
      "destination": "/linuxcash/logs/archive/logs",
      "transform": "l-%(cashcode)s-%(shift)s"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/current",
      "mask": "*",
      "action": "move",
      "destination": "/linuxcash/logs/cashlogs",
      "transform": "%(shift)s/%(srcname)s"
    }
  ],
  "weekly": [
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/cashlogs/*",
      "rangetype": "exceptlast",
      "rangecount": 8,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/cash/data/backup",
      "rangetype": "exceptlast",
      "rangecount": 30,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/archive/dicts/*.*",
      "rangetype": "exceptlast",
      "rangecount": 60,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/archive/nbackup/*.*",
      "rangetype": "exceptlast",
      "rangecount": 3,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/archive/logs/l*.zip",
      "rangetype": "exceptlast",
      "rangecount": 360,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/archive/*.*sec",
      "rangetype": "exceptlast",
      "rangecount": 360,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/archive/*.*sk",
      "rangetype": "exceptlast",
      "rangecount": 360,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/archive/*.*z",
      "rangetype": "exceptlast",
      "rangecount": 360,
      "action": "delete"
    },
    {
      "objectname": "directory",
      "path": "/linuxcash/logs/archive/*.*st",
      "rangetype": "exceptlast",
      "rangecount": 360,
      "action": "delete"
    }
  ],
  "manual":
    {
      "outputDir": "/linuxcash/logs/backup/",
      "shiftCount": 360,
      "timeout": 240
    }
}
